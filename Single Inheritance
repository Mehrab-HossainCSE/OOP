#include <iostream>
using namespace std;
class pavel
{  public:
   void method()
   {
       cout<<"This is parent class"<<endl;
   }
};
class emon: public pavel{
   public:
   void function()
   {
       cout<<"This is child class"<<endl;
   }
} ;
int main()
{

    emon obj1;
    obj1.function();
    obj1.method();
    return 0;
}
