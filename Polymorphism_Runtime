#include<bits/stdc++.h>
using namespace std;
class Animal
{
public:
    void func()
    {
        cout<<"It is a Animal class"<<endl;

    }



};
class Dog: public Animal
{
public:
    void func()
    {
        cout<<"It is a dog class"<<endl;

    }

};
int main()
{
    Dog obj;

    obj.func();

    return 0;
}
