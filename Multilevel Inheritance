#include<bits/stdc++.h>
using namespace std;
class A{
    public:
    void funcA(){
    cout<<"Base class A"<<endl;
    }
};
class B: public A{
    public:
    void funcB(){
    cout<<"Base class B"<<endl;
    }
};
class C : public B{

    public:
    void funcC()
    {

        cout<<"Base class C"<<endl;

    }

};
int main()
{
    C obj1;
    obj1.funcA();
    obj1.funcB();
    obj1.funcC();
    //obj1.funcB();

    return 0;
}
