#include<bits/stdc++.h>
using namespace std;
class A
{
public:
    void funcA()
    {
        cout<<"Base class A"<<endl;
    }
};
class B: public A
{
public:
    void funcB()
    {
        cout<<"Base class B Inherited class A"<<endl;
    }
};
class C : public A
{

public:
    void funcC()
    {

        cout<<"Base class C Inherited class A"<<endl;

    }

};
int main()
{
    C obj1;
    obj1.funcC();
    obj1.funcA();


    //obj1.funcB();
    B obj2;
    obj2.funcB();
    obj2.funcA();

    return 0;
}
